<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WebTool.AppShell"
             xmlns:styles="clr-namespace:WebTool.Styles"
             BackgroundColor="{DynamicResource DarkColor}"
             FlyoutBackgroundColor="{DynamicResource DarkColor}"
             ForegroundColor="{DynamicResource DarkColor}"
             FlyoutHeaderBehavior="CollapseOnScroll">
    <Shell.FlyoutHeader>
        <styles:FlyoutHeader/>
    </Shell.FlyoutHeader>

</Shell>
